{
  "name": "nextjs-starter-chakra-ui",
  "version": "0.0.0",
  "private": true,
  "author": "Rohman Habib M <mhrohman@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "export": "next export",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true next build",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "cm": "cz --disable-emoji",
    "clean-build": "rm -rf out .next*",
    "gen:theme-typings": "chakra-cli tokens ./src/theme.ts",
    "postinstall": "husky install && npm run gen:theme-typings",
    "release": "standard-version"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.9",
    "@chakra-ui/styled-system": "^1.16.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": ">=4.1.17",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/bundle-analyzer": "^12.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-cz": "^4.8.0",
    "husky": "7.0.4",
    "lint-staged": "^12.2.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.5",
    "webpack": "5.67.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
