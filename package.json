{
  "name": "nextjs-starter-chakra-ui",
  "version": "0.0.0",
  "private": true,
  "author": "Rohman Habib M <mhrohman@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "export": "next export",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true next build",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "cm": "cz --disable-emoji",
    "clean-build": "rm -rf out .next*",
    "gen:theme-typings": "chakra-cli tokens ./src/theme.ts",
    "postinstall": "husky install && npm run gen:theme-typings",
    "release": "standard-version"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "framer-motion": ">=4.1.9",
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.3.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@next/bundle-analyzer": "^10.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^5.0.0",
    "git-cz": "^4.7.6",
    "husky": "6.0.0",
    "lint-staged": "^10.5.4",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.2.15",
    "prettier": "^2.2.1",
    "sass": "^1.32.11",
    "standard-version": "^9.2.0",
    "typescript": "^4.2.4",
    "webpack": "5.35.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
