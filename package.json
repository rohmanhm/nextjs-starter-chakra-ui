{
  "name": "nextjs-starter-chakra-ui",
  "version": "0.0.0",
  "private": true,
  "author": "Rohman Habib M <mhrohman@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "export": "next export",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true next build",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "cm": "cz --disable-emoji",
    "clean-build": "rm -rf out .next*",
    "gen:theme-typings": "chakra-cli tokens ./src/theme.ts",
    "lint": "next lint",
    "postinstall": "husky install && npm run gen:theme-typings",
    "release": "standard-version"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.9",
    "@chakra-ui/styled-system": "^1.16.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": ">=4.1.17",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@next/eslint-plugin-next": "^12.3.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "next-pwa": "^5.6.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.4",
    "webpack": "5.74.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
